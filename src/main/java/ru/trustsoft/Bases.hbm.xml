<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.trustsoft.Bases" table="bases" schema="tsmngr">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="basename">
            <column name="basename" sql-type="text"/>
        </property>
        <property name="description">
            <column name="description" sql-type="text" not-null="true"/>
        </property>
        <many-to-one name="contragentsByContragentid" class="ru.trustsoft.Contragents">
            <column name="contragentid"/>
        </many-to-one>
        <set name="basesofusersById" inverse="true">
            <key>
                <column name="baseid"/>
            </key>
            <one-to-many not-found="ignore" class="ru.trustsoft.Basesofusers"/>
        </set>
    </class>
</hibernate-mapping>