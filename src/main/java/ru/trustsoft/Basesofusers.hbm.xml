<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.trustsoft.Basesofusers" table="basesofusers" schema="tsmngr">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <many-to-one name="usersByUserid" class="ru.trustsoft.Users">
            <column name="userid"/>
        </many-to-one>
        <many-to-one name="basesByBaseid" class="ru.trustsoft.Bases">
            <column name="baseid"/>
        </many-to-one>
    </class>
</hibernate-mapping>